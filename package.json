{
  "name": "ts-node-template",
  "version": "1.0.0",
  "description": "template for node typescript api.",
  "main": "src/app.ts",
  "scripts": {
    "build": "npx prisma generate && npx tsc",
    "start": "yarn run build && yarn node dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf generated && rm -rf node_modules",
    "winclean": "powershell -Command \"Remove-Item -Recurse -Force dist, generated, node_modules"
  },
  "author": "Raymond Lyon",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.6.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^4.17.1",
    "express-rate-limit": "^7.5.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2"
  }
}
